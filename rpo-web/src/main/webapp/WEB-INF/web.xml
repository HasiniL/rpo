    <!--</servlet>-->
    <!--<servlet-mapping>-->
        <!--<servlet-name>rps-show.jsp</servlet-name>-->
        <!--<url-pattern>/</url-pattern>-->
    <!--</servlet-mapping>-->

    <error-page>
        <exception-type>java.lang.Throwable</exception-type>
        <location>/errors.jsp</location>
    </error-page>

</web-app>
